{
  "name": "coinmarketcap-exchangerate",
  "version": "1.0.0",
  "description": "A TypeScript module to fetch cryptocurrency prices from CoinMarketCap API and forex prices from ExchangeRate API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "bundle": "terser dist/index.js -o dist/bundle.min.js -c -m",
    "prepare": "npm run build && npm run bundle",
    "test": "jest",
    "dev": "ts-node src/index.ts",
    "example": "ts-node example.ts"
  },
  "keywords": [
    "coinmarketcap",
    "exchangerate",
    "cryptocurrency",
    "forex",
    "prices",
    "api"
  ],
  "author": "Droonacharya Ko Chelo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/droonacharyakochelo/cmc_er.git"
  },
  "bugs": {
    "url": "https://github.com/droonacharyakochelo/cmc_er/issues"
  },
  "homepage": "https://github.com/droonacharyakochelo/cmc_er#readme",
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.8.0",
    "jest": "^29.7.0",
    "terser": "^5.44.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.0",
    "typescript": "^5.2.0"
  },
  "files": [
    "dist/**/*",
    "src/data/exchange-rate-currencies.json"
  ]
}
